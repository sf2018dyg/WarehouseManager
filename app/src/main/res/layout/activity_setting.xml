<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_color"
    android:orientation="vertical">

    <include layout="@layout/view_title_layout" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/line"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="提示音"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/dp_10"
            android:background="@color/white"/>

        <Space
            android:layout_width="match_parent"
            android:layout_height="1dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@color/white">

            <ImageView
                android:id="@+id/imgfSuccess"
                android:layout_width="22dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_14"
                android:layout_marginRight="@dimen/dp_6"
                android:adjustViewBounds="true"
                android:src="@mipmap/setting_menu1" />

            <TextView
                android:id="@+id/text1"
                style="@style/text_14sp_black_style"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imgfSuccess"
                android:text="@string/setting_music_complete" />

            <RelativeLayout
                android:id="@+id/llfSMToSucess"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/text1"
                android:gravity="center_vertical|right"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvfSMToSucess"
                    style="@style/text_14sp_gray_style"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginRight="4dp"
                    android:layout_toLeftOf="@+id/image1"
                    android:ellipsize="end"
                    android:hint="@string/setting_music_select"
                    android:maxLines="2"
                    android:gravity="right" />

                <ImageView
                    android:id="@+id/image1"
                    android:layout_width="@dimen/dp_10"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dp_10"
                    android:adjustViewBounds="true"
                    android:src="@mipmap/arrow_right" />
            </RelativeLayout>
        </RelativeLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="1dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@color/white">

            <ImageView
                android:id="@+id/imgfRe"
                android:layout_width="22dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_14"
                android:layout_marginRight="@dimen/dp_6"
                android:adjustViewBounds="true"
                android:src="@mipmap/setting_menu2" />

            <TextView
                android:id="@+id/text2"
                style="@style/text_14sp_black_style"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imgfRe"
                android:text="@string/setting_music_repetition" />

            <RelativeLayout
                android:id="@+id/llfSMToRepetition"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/text2"
                android:gravity="center_vertical|right"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvfSMToRepetition"
                    style="@style/text_14sp_gray_style"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginRight="4dp"
                    android:layout_toLeftOf="@+id/image2"
                    android:ellipsize="end"
                    android:hint="@string/setting_music_select"
                    android:maxLines="2"
                    android:gravity="right"/>

                <ImageView
                    android:id="@+id/image2"
                    android:layout_width="@dimen/dp_10"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dp_10"
                    android:adjustViewBounds="true"
                    android:src="@mipmap/arrow_right" />
            </RelativeLayout>
        </RelativeLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="1dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@color/white">

            <ImageView
                android:id="@+id/imgfFail"
                android:layout_width="22dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_14"
                android:layout_marginRight="@dimen/dp_6"
                android:adjustViewBounds="true"
                android:src="@mipmap/setting_menu3" />

            <TextView
                android:id="@+id/text3"
                style="@style/text_14sp_black_style"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imgfFail"
                android:text="@string/setting_music_fail" />

            <RelativeLayout
                android:id="@+id/llfSMToFail"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/text3"
                android:gravity="center_vertical|right"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvfSMToFail"
                    style="@style/text_14sp_gray_style"
                    android:layout_toLeftOf="@+id/image3"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginRight="4dp"
                    android:ellipsize="end"
                    android:hint="@string/setting_music_select"
                    android:maxLines="2"
                    android:gravity="right"
                    />

                <ImageView
                    android:id="@+id/image3"
                    android:layout_width="@dimen/dp_10"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dp_10"
                    android:adjustViewBounds="true"
                    android:src="@mipmap/arrow_right" />
            </RelativeLayout>
        </RelativeLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="1dp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_10" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:text="打印设置"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/dp_10"
            android:background="@color/white"/>

        <Space
            android:layout_width="match_parent"
            android:layout_height="1dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@color/white">

            <ImageView
                android:id="@+id/imgfPrint"
                android:layout_width="22dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_14"
                android:layout_marginRight="@dimen/dp_6"
                android:adjustViewBounds="true"
                android:src="@mipmap/print_icon" />

            <TextView
                android:id="@+id/text4"
                style="@style/text_14sp_black_style"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imgfPrint"
                android:text="打印机" />

            <RelativeLayout
                android:id="@+id/llfSMToPrint"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/text4"
                android:gravity="center_vertical|right"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvfSMToPrint"
                    style="@style/text_14sp_gray_style"
                    android:layout_toLeftOf="@+id/image4"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginRight="4dp"
                    android:ellipsize="end"
                    android:hint="请选择打印机"
                    android:maxLines="2"
                    android:gravity="right"
                    />

                <ImageView
                    android:id="@+id/image4"
                    android:layout_width="@dimen/dp_10"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dp_10"
                    android:adjustViewBounds="true"
                    android:src="@mipmap/arrow_right" />
            </RelativeLayout>
        </RelativeLayout>


        <Space
            android:layout_width="match_parent"
            android:layout_height="1dp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@color/white">

            <ImageView
                android:id="@+id/imgfPrintPlan"
                android:layout_width="22dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_14"
                android:layout_marginRight="@dimen/dp_6"
                android:adjustViewBounds="true"
                android:src="@mipmap/form_icon" />

            <TextView
                android:id="@+id/text5"
                style="@style/text_14sp_black_style"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/imgfPrintPlan"
                android:text="打印方案" />

            <RelativeLayout
                android:id="@+id/llfSMToPrintPlan"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/text5"
                android:gravity="center_vertical|right"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvfSMToPrintPlan"
                    style="@style/text_14sp_gray_style"
                    android:layout_toLeftOf="@+id/image5"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginRight="4dp"
                    android:ellipsize="end"
                    android:hint="请选择打印方案"
                    android:maxLines="2"
                    android:gravity="right"
                    />

                <ImageView
                    android:id="@+id/image5"
                    android:layout_width="@dimen/dp_10"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dp_10"
                    android:adjustViewBounds="true"
                    android:src="@mipmap/arrow_right" />
            </RelativeLayout>
        </RelativeLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="1dp" />

    </LinearLayout>

</LinearLayout>